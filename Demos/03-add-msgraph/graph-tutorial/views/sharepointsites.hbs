<h1>SharePoint Sites</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Domain</th>
      <th scope="col">Sites</th>
      <th scope="col">webUrl</th>
      <th scope="col">Created</th>
    </tr>
  </thead>
  <tbody>
    {{#each sites}}
      <tr>
        <td>{{this.siteCollection.hostname}}</td>
        <td>{{this.displayName}}</td>
        <td>{{this.webUrl}}</td>
        <td>{{this.createdDateTime}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
